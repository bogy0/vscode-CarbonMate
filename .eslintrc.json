{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "@typescript-eslint/naming-convention": "warn",
    "@typescript-eslint/semi": "warn",
    "@typescript-eslint/member-delimiter-style": "warn",
    "prefer-const": "warn"
  },
  "ignorePatterns": [
    "out",
    "dist",
    "**/*.d.ts"
  ]
} 